{
  "name": "Universal - Latest",
  "image": "mcr.microsoft.com/devcontainers/universal:latest",
  "postCreateCommand": "bash .devcontainer/postCreate.sh"
  "features": {
    "ghcr.io/oleksis/devcontainer-features/python-launcher:1": {
      "version": "1.0.0"
    },
    "ghcr.io/raucha/devcontainer-features/pytorch:1": {},
    "ghcr.io/davzucky/devcontainers-features-wolfi/python:1": {
      "installRuff": true,
      "installUV": true,
      "pythonVersion": "3.10"
    },
    "ghcr.io/roul/devcontainer-features/mise-python:1": {},
    "ghcr.io/itsalljustdata/devcontainers-custom-features/python-virtualenv:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.debugpy",
        "ms-python.python"
      ]
    }
  }
}
